{
  "name": "electric-tanstack-db",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --port 5174",
    "dev:api": "bun run src/server/index.ts",
    "dev:electric": "docker compose -f docker/docker-compose.yml up",
    "dev:caddy": "caddy run --config Caddyfile",
    "dev:all": "concurrently -n electric,api,app,caddy -c blue,magenta,green,yellow \"bun run dev:electric\" \"bun run dev:api\" \"bun run dev\" \"bun run dev:caddy\"",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@electric-sql/client": "^1.0.0-beta.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/db": "^0.0.7",
    "@tanstack/db-collections": "^0.0.7",
    "@tanstack/react-router": "^1.93.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/router-plugin": "^1.145.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.3",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "hono": "^4.11.3",
    "lucide-react": "^0.562.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.32"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
